<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox alignment="CENTER_LEFT" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.NavbarController">
    <Button fx:id="backButton" focusTraversable="false" mnemonicParsing="false" onAction="#handleBackButton" text="&lt;">
        <font>
            <Font name="Symbol" size="12.0" />
        </font>
    </Button>
    <Button fx:id="inventoryButton" focusTraversable="false" mnemonicParsing="false" onAction="#handleInventoryButton" prefWidth="105.0" text="Inventory" />
    <Button fx:id="cashRegistersButton" focusTraversable="false" mnemonicParsing="false" onAction="#handleCashRegistersButton" prefWidth="115.0" text="Cash Registers" />
    <Button fx:id="transactionsButton" focusTraversable="false" mnemonicParsing="false" onAction="#handleTransactionsButton" prefWidth="105.0" text="Transactions" />
</HBox>
