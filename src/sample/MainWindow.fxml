<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="CENTER" hgap="10" prefHeight="300.0" prefWidth="450.0" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainWindowController">

    <Button fx:id="inventoryButton" focusTraversable="false" onAction="#handleInventoryButton" prefHeight="100.0" prefWidth="100.0" text="Inventory" textAlignment="CENTER">
      <font>
         <Font size="14.0" />
      </font></Button>
   <Button fx:id="cashRegistersButton" disable="true" focusTraversable="false" onAction="#handleCashRegistersButton" prefHeight="100.0" prefWidth="100.0" text="Cash Registers" textAlignment="CENTER" wrapText="true" GridPane.columnIndex="1">
      <font>
         <Font size="14.0" />
      </font></Button>
   <Button fx:id="transactionsButton" disable="true" focusTraversable="false" onAction="#handleTransactionsButton" prefHeight="100.0" prefWidth="100.0" text="Transactions" textAlignment="CENTER" GridPane.columnIndex="2">
      <font>
         <Font size="14.0" />
      </font></Button>
   <Button fx:id="databaseButton" mnemonicParsing="false" onAction="#handleDatabaseButton" prefWidth="100.0" text="Disconnect DB" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints minWidth="10.0" />
      <ColumnConstraints minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
